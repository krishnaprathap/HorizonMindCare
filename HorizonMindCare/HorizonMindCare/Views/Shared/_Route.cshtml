
<div class="row">
    <div class="col-lg-12 col-md-12 col-sm-12 col-sm-12">
        <script src='https://maps.googleapis.com/maps/api/js?v=3.exp&sensor=false&key=AIzaSyCwVskqxpPiPoggBOzpdTWV8GVCiY7JIL4'></script>
        <div style='overflow:hidden;height:275px;width:1400px;'>
            <div id='gmap_canvas' style='height:275px;width:1400px;'></div>
            <div><small><a href="http://embedgooglemaps.com">embed google maps</a></small></div>
            <div><small><a href="https://enablecookies.info/">enable cookies with this guide how to</a></small></div>
            <style>
                #gmap_canvas img {
                    max-width: none !important;
                    background: none !important;
                }
            </style>
        </div>
        <script type='text/javascript'>
                function init_map() {
                    var myOptions = {
                        zoom: 10,
                        center: new google.maps.LatLng(16.717523701780642, 74.26093165541693),
                        mapTypeId: google.maps.MapTypeId.ROADMAP
                    };
                    map = new google.maps.Map(document.getElementById('gmap_canvas'), myOptions);
                    marker = new google.maps.Marker({
                        map: map,
                        position: new google.maps.LatLng(16.717523701780642, 74.26093165541693)
                    });
                    infowindow = new google.maps.InfoWindow({
                        content: '<strong>Horizon Mind Care Clinic</strong><br>Royal Classic E ward Rajarampuri 3rd lane Kolhapur 416008<br>'
                    });
                    google.maps.event.addListener(marker, 'click', function() {
                        infowindow.open(map, marker);
                    });
                    infowindow.open(map, marker);
                }
                google.maps.event.addDomListener(window, 'load', init_map);
        </script>
    </div>
</div>