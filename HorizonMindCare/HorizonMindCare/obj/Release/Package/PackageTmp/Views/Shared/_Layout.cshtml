@using System.Web.Optimization
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="content-type" content="text/html; charset=utf-8">
    <meta name="author" content="Horizon">
    <title>Horizon Mind Care Clinic</title>
    <link rel="shortcut icon" href="~/Content/img/favicon.png">
    <link rel="stylesheet" href="~/Content/css/bootstrap.min.css" type="text/css">
    <link rel="stylesheet" href="~/Content/fonts/font-awesome.min.css" type="text/css">
    <link rel="stylesheet" href="~/Content/fonts/themify-icons.css">
    <link rel="stylesheet" href="~/Content/css/main.css" type="text/css">
    @Styles.Render("~/Content/allcss")
</head>

<body>
    @Html.Partial("_Menu")
    @Html.Partial("_aboutDoctor")
    @Html.Partial("_Equipments")
    @Html.Partial("_testimonial")
    @Html.Partial("_Route")
    @Html.Partial("_contactForm")
    @Html.Partial("_facilities")
    @RenderBody()
    @Html.Partial("_footer")

    <script type="text/javascript" src="~/Scripts/jquery-min.js"></script>
    <script language="JavaScript" type="text/javascript">
        $(document).ready(function () {
            $('.carousel').carousel({
                interval: 2000
            })
        });

    </script>
    @Scripts.Render("~/Content/alljs")
    <script src="~/Scripts/Extra/Personal.js"></script>
    <script type="text/javascript" src="~/Scripts/Extra/owl.carousel.min.js"></script>
    <script type="text/javascript" src="~/Scripts/Extra/main.js"></script>
</body>
</html>


